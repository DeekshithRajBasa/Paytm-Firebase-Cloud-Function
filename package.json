{
    "name": "functions",
    "author": "Abdul Ghani",
    "description": "Cloud Function for Generating Paytm Txn Token!",
    "scripts": {
        "lint": "eslint .",
        "serve": "firebase emulators:start --only functions",
        "shell": "firebase functions:shell",
        "start": "npm run shell",
        "deploy": "firebase deploy -P default --only functions:paytmTxnToken",
        "deploy-prod": "firebase deploy -P prod --only functions:paytmTxnToken",
        "logs": "firebase functions:log"
    },
    "engines": {
        "node": "10"
    },
    "main": "index.js",
    "dependencies": {
        "firebase-admin": "^8.10.0",
        "firebase-functions": "^3.6.1",
        "axios": "^0.20.0"
    },
    "devDependencies": {
        "eslint": "^5.12.0",
        "eslint-plugin-promise": "^4.0.1",
        "firebase-functions-test": "^0.2.0"
    },
    "private": true
}
